import java.util.Scanner;
class main{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter Number: ");
        Integer s= sc.nextInt();
        if(s<0){
            System.out.print("Enter Valid number");
        }else{
            int f=1;
            for(int i=1;i<=s;i++){
                f*=i;
            }
        System.out.print(f);
        }
    }
}
