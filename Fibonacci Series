import java.util.Scanner;
class main{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter number:");
        int a=sc.nextInt();
        int ft=0;
        int st=1;
        System.out.print("Fibonacci Series= ");
        for(int i=1;i<=a;i++){
            System.out.print(ft+ " ");
            int nt=ft+st;
            ft=st;
            st=nt;
        }
        
    }
}
